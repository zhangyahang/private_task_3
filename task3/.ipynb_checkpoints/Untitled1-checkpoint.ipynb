   "source": [
    "def weird_gen(s):\n",
    "    s = [s]\n",
    "    while s:\n",
    "        *x, a = s[0].split(',', 2)\n",
    "        y, *s = a.split('\\n', 1)\n",
    "        yield ', '.join(z.strip().replace('\\n', ' ') for z in x + [y])\n",
    "\n",
    "print('\\n'.join(weird_gen(open('reviews_detail.csv','rb').read())))"
   ]
